{
  "name": "express-rest-api-shop",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app[.js",
  "modulesUrls": {
    "nodemon": "./node_modules/nodemon/bin/nodemon",
    "knex": "./node_modules/knex/bin/cli.js",
    "typescript": "./node_modules/typescript/bin/tsc",
    "eslint": "./node_modules/eslint/bineslint.js"
  },
  "knexfile": "--cwd src/database",
  "scripts": {
    "typescript": "./node_modules/typescript/bin/tsc",
    "ts": "npm run typescript -- -w",
    "start": "node dist/app.js",
    "dev-server": "node $npm_package_modulesUrls_nodemon dist/app.js",
    "init-knexfile": "$npm_package_modulesUrls_knex init ",
    "migrate:create": "npx ts-node $npm_package_modulesUrls_knex migrate:make $NAME $npm_package_knexfile",
    "migrate:latest": "npx ts-node $npm_package_modulesUrls_knex migrate:latest $npm_package_knexfile",
    "migrate:rollback": "npx ts-node $npm_package_modulesUrls_knex migrate:rollback $npm_package_knexfile",
    "migrate:down": "npx ts-node $npm_package_modulesUrls_knex migrate:down $npm_package_knexfile",
    "migrate:up": "npx ts-node $npm_package_modulesUrls_knex migrate:up $npm_package_knexfile",
    "seed:create": "npx ts-node $npm_package_modulesUrls_knex seed:make $NAME $npm_package_knexfile",
    "seed:run": "npx ts-node $npm_package_modulesUrls_knex seed:run $npm_package_knexfile",
    "eslint": "./node_modules/eslint/bin/eslint.js",
    "eslint:init": "npm run eslint -- --init",
    "eslint:run": "npm run eslint -- ./src/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/passport": "^1.0.2",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-local": "^1.0.33",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "knex": "^0.20.8",
    "log4js": "^6.1.2",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.17.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.4"
  }
}
